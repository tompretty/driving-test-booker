{
	"name": "driving-test-booker",
	"version": "0.0.0",
	"description": "A project for booking a late cancellation driving test",
	"repository": {
		"type": "git",
		"url": "https://github.com/tompretty/driving-test-booker"
	},
	"license": "MIT",
	"author": {
		"email": "tompretty2@gmail.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"start": "node --import ./ts-node.register.mjs ./src/index.ts",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@playwright/browser-firefox": "^1.45.2",
		"playwright": "^1.45.2"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
		"@eslint/js": "^9.7.0",
		"@release-it/conventional-changelog": "^8.0.1",
		"@types/eslint-plugin-markdown": "^2.0.2",
		"@vitest/coverage-v8": "^2.0.3",
		"console-fail-test": "^0.4.4",
		"cspell": "^8.11.0",
		"eslint": "^9.7.0",
		"eslint-plugin-jsdoc": "^48.7.0",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-markdown": "^5.1.0",
		"eslint-plugin-n": "^17.9.0",
		"eslint-plugin-package-json": "^0.15.2",
		"eslint-plugin-perfectionist": "^2.11.0",
		"eslint-plugin-regexp": "^2.6.0",
		"eslint-plugin-vitest": "^0.5.4",
		"eslint-plugin-yml": "^1.14.0",
		"husky": "^9.1.1",
		"jsonc-eslint-parser": "^2.4.0",
		"knip": "^5.26.0",
		"lint-staged": "^15.2.7",
		"markdownlint": "^0.34.0",
		"markdownlint-cli": "^0.41.0",
		"prettier": "^3.3.3",
		"prettier-plugin-curly": "^0.2.2",
		"prettier-plugin-packagejson": "^2.5.1",
		"prettier-plugin-sh": "^0.14.0",
		"release-it": "^17.6.0",
		"sentences-per-line": "^0.2.1",
		"ts-node": "^10.9.2",
		"tsup": "^8.2.0",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.16.1",
		"vitest": "^2.0.3"
	},
	"packageManager": "pnpm@8.9.0+sha512.ef8859938e1f0537b9fcf0309044311392f3b38b344176e6cd4fdee21b225524b0ac2a1b08ae2ae2ddd5b3ff4b8ca496d6778e950f3e7c9f555b81325982441c",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
